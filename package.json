{
  "name": "bitmap-min",
  "version": "0.0.3",
  "description": "基于canvas的轻量级浏览器端位图处理插件 (gif, png, jpeg)",
  "main": "dist/bitmap-min.js",
  "scripts": {
    "test": "jest",
    "watch": "FORMAT=iife rollup -c -w",
    "browser": "browser-sync start --config bs-config.js",
    "dev": "npm-run-all --parallel watch browser",
    "build": "rm -rf dist && rollup -c && FORMAT=iife rollup -c && FORMAT=umd rollup -c",
    "babel": "babel src -d lib --plugins transform-es2015-modules-commonjs",
    "prepublishOnly": "npm-run-all --parallel build babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zsl99a/bitmap-min.git"
  },
  "keywords": [
    "gif",
    "png",
    "jpeg",
    "browser",
    "compress",
    "cut",
    "head",
    "portrait",
    "china"
  ],
  "author": "清酒稻香",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zsl99a/bitmap-min/issues"
  },
  "homepage": "https://github.com/zsl99a/bitmap-min#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "browser-sync": "^2.18.13",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1"
  }
}
